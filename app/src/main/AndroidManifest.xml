<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="com.android.alarm.permission.SET_ALARM"/>
    <uses-permission android:name="android.permission.INTERNET"/>


    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/dreaming"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.HelloAppsTI22A4">

        <receiver android:name="com.example.syahdan.AlarmReceiver"></receiver>

        <activity
            android:name="com.example.syahdan.MainLauncherSplashLogo"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

            <activity android:name="com.example.syahdan.MapActivity"
                android:exported="true">
                <intent-filter>
                    <action android:name="android.intent.action.VIEW" />
                    <data android:scheme="geo" />
                    <category android:name="android.intent.category.DEFAULT" />
                </intent-filter>
            </activity>


        <activity android:name="com.example.syahdan.MainHomePage"/>

        <activity
            android:name="com.example.syahdan.MainHello"
            android:exported="false">
        </activity>
        <activity
            android:name="com.example.syahdan.ScrollingIcecold"
            android:exported="false">
        </activity>
        <activity
            android:name="com.example.syahdan.MainActivityOne"
            android:exported="false">
        </activity>
        <activity
            android:name="com.example.syahdan.SecondActivity"
            android:exported="false">
        </activity>
        <activity
            android:name="com.example.syahdan.MainActivity"
            android:exported="false">
        </activity>
        <activity
            android:name="com.example.syahdan.MainCount"
            android:exported="false">
        </activity>
        <activity
            android:name="com.example.syahdan.AlarmActivity"
            android:exported="false">
        </activity>
        <activity
            android:name="com.example.syahdan.TabActivity"
            android:exported="false">
        </activity>
        <activity
            android:name="com.example.syahdan.Exoplayer1Activity"
            android:exported="false">
        </activity>
        <activity
            android:name="com.example.syahdan.Exoplayer2Activity"
            android:exported="false">
        </activity>


    </application>

</manifest>

